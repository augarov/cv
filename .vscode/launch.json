{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cv_renderer - latex - debug",
      "type": "debugpy",
      "request": "launch",
      "module": "cv_renderer",
      "args": [
        "--log-level",
        "DEBUG",
        "--data",
        "cv_data.yaml",
        "--input",
        "templates/cv.tex.j2",
        "--output",
        "build/debug/cv.tex",
        "--force"
      ]
    },
    {
      "name": "cv_renderer - html - debug",
      "type": "debugpy",
      "request": "launch",
      "module": "cv_renderer",
      "args": [
        "--log-level",
        "DEBUG",
        "--data",
        "cv_data.yaml",
        "--input",
        "templates/cv.html.j2",
        "--output",
        "build/debug/cv.html",
        "--force"
      ]
    }
  ]
}
