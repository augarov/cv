{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cv_renderer - latex - debug",
      "type": "debugpy",
      "request": "launch",
      "module": "cv_renderer",
      "args": [
        "--log-level",
        "DEBUG",
        "--data",
        "cv_data.yaml",
        "--input",
        "templates/tex/cv-deedy.tex.j2",
        "--output",
        "build/debug/cv-deedy.tex",
        "--force"
      ]
    },
    {
      "name": "cv_renderer - html - debug",
      "type": "debugpy",
      "request": "launch",
      "module": "cv_renderer",
      "args": [
        "--log-level",
        "DEBUG",
        "--data",
        "cv_data.yaml",
        "--input",
        "templates/web/index.html.j2",
        "--output",
        "build/debug/index.html",
        "--force"
      ]
    }
  ]
}
